{
  "name": "react-native-chat-sdk",
  "version": "1.3.2",
  "summary": "react native chat sdk",
  "homepage": "https://github.com/AgoraIO/Agora-Chat-API-Examples",
  "license": "MIT",
  "authors": "Asterisk <zuoyu@agora.io> (https://github.com/asteriskzuo)",
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/easemob/react-native-chat-sdk.git",
    "tag": "1.3.2"
  },
  "source_files": [
    "ios/**/*.{h,m,mm}",
    "native_src/cpp/**/*.{h,cpp,mm}",
    "native_src/objc/**/*"
  ],
  "private_header_files": [
    "native_src/cpp/**/*.h"
  ],
  "exclude_files": [
    "native_src/cpp/java/**/*",
    "native_src/cpp/android/**/*",
    "native_src/objc/flutter/**/*"
  ],
  "dependencies": {
    "React-Core": [],
    "HyphenateChat": [
      "4.10.2"
    ],
    "React-Codegen": [],
    "RCT-Folly": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/core": []
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "GCC_PREPROCESSOR_DEFINITIONS": [
      "OBJC_LANGUAGE",
      "REACT_NATIVE_ARCHITECTURE",
      "IOS_PLATFORM"
    ]
  },
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -Wno-comma -Wno-shorten-64-to-32 -DRCT_NEW_ARCH_ENABLED=1",
  "pod_target_xcconfig": {
    "OTHER_CPLUSPLUSFLAGS": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "HEADER_SEARCH_PATHS": [
      "$(PODS_ROOT)/boost",
      "$(PODS_TARGET_SRCROOT)/native_src/cpp/common",
      "$(PODS_TARGET_SRCROOT)/native_src/cpp/core",
      "$(PODS_TARGET_SRCROOT)/native_src/cpp/objc",
      "$(PODS_TARGET_SRCROOT)/native_src/objc/common",
      "$(PODS_TARGET_SRCROOT)/native_src/objc/dispatch",
      "$(PODS_TARGET_SRCROOT)/native_src/objc/rn"
    ]
  }
}
